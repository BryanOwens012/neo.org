        </div>
    </li>
</ul>

<div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="clientModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <img id="model_img" />
                <div>
                    <h2 class="modal-title" id="model_name"></h2>
                    <div class="developer" id="model_dev"></div>
                </div>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="model_desc"></div>
            <div class="modal-footer" id="model_link"></div>
        </div>
    </div>
</div>

@section Scripts{
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $(".client-ul .client-logo").click(function () {
                zoomin(this);
            });
            $(".card-header a").attr("onclick", "showDetails(this)");
        })
        function showDetails(obj){
            var col = $(obj).parent().parent().find('.collapse');
            $(".collapse").not(col).hide('fast');
            col.toggle('fast');
        }
        function showPlatform(obj, name){
            if (name) {
                $('.client-ul li').hide();
                $('.' + name).show();
            }
            else
                $('.client-ul li').show();
            $('.nav-client li').removeClass("selected");
            $(obj).addClass("selected");
        }
        function zoomin(obj){
            var img = $(obj).parent().find("img");
            var name = $(obj).parent().find("header");
            var developer = $(obj).parent().find(".developer");
            var desc = $(obj).parent().find(".desc");
            var link = $(obj).parent().find(".link");
            $("#modal").modal();
            $("#model_img").html("");
            $("#model_name").html("");
            $("#model_dev").html("");
            $("#model_desc").html("");
            $("#model_link").html("");
            desc.find("img").each(function () {
                $(this).attr("src", $(this).data("src"));
            });;

            $("#model_img").attr("src", img.attr("src"));
            $("#model_name").html(name.html());
            $("#model_dev").html(developer.html());
            $("#model_desc").html(desc.html());
            $("#model_link").html(link.html());
        }
        $("#menu_list a").css("border-bottom", "none")
        $("#menu_list li.nav-item [href='/client']").css("border-bottom", "2px solid #58bf00")
    </script>
}