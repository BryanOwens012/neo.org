    <li onclick="showPlatform(this, 'mobile')"><i class="iconfont">&#xe64b;</i><span>@Localizer["Mobile"]</span></li>
    <li onclick="showPlatform(this, 'web')"><i class="iconfont">&#xe63c;</i><span>@Localizer["Web"]</span></li>
